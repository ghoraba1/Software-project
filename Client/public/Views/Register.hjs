    .form-container {
      background-color: #fff;
      padding: 20px;
      border-radius: 20px;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
      max-width: 700px;
      margin: 70px auto;
    }

    #errorMessage {
      color: red;
